{
  "homepage": "/",
  "scripts": {
    "start": "tsc && yarn dev",
    "build": "tsc && vite build -c config/vite.config.ts",
    "dev": "vite -c config/vite.config.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --config config/eslint.config.js --fix",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx src --config config/eslint.config.js",
    "lint:contracts": "solium --fix --dir ./contracts",
    "test": "cypress open-ct",
    "test:ci": "cypress run-ct --quiet --reporter spec",
    "test:integration": "concurrently \"yarn start \" \"cypress open\"",
    "test:integration:ci": "cypress run --headless",
    "compile:contracts": "truffle compile"
  },
  "dependencies": {
    "@3id/connect": "^0.1.6",
    "@ceramicnetwork/3id-did-resolver": "^1.2.7",
    "@ceramicnetwork/http-client": "^1.0.7",
    "@ceramicstudio/idx": "^0.12.1",
    "@headlessui/react": "^1.4.0",
    "@textile/hub": "^6.2.0",
    "dids": "^2.4.0",
    "ethers": "^5.4.4",
    "i18next": "^19.9.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^11.8.8",
    "react-router-dom": "^5.2.0",
    "vite-compatible-simple-peer": "^9.11.0",
    "web3": "^1.5.2",
    "web3.storage": "^3.2.0",
    "web3modal": "^1.9.4",
    "zustand": "^3.2.0"
  },
  "devDependencies": {
    "@cypress/react": "^5.7.0",
    "@cypress/vite-dev-server": "^1.2.7",
    "@openzeppelin/contracts": "^4.2.0",
    "@react-three/test-renderer": "^6.1.1",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/react": "^11.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^9",
    "concurrently": "^6.0.0",
    "cypress": "^7.7.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "moduloze": "^0.6.2",
    "postcss": "^7",
    "prettier": "^2.3.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.3.5",
    "vite": "^2.1.2",
    "vite-aliases": "^0.5.1"
  }
}
